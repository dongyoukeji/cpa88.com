.search span{display: block; float: left; border-left: 1px solid #DDD; padding-left: 15px; margin-left: 10px;}
#check1{background: #29A7E2; border-radius: 5px; color: #FFF;}
#check2{background: #555; border-radius: 5px; color: #FFF;}
.search span a{display: block; float: left; color: #545454; font-weight: bold; padding: 5px 10px; margin-right: 5px;}
.search span b{padding: 5px 10px; display: block; float: left; font-weight: bold; color: #545454;}
.search span b i{color: #29A7E2; font-weight: bold;}
.search span b em{color: #FF0000; font-weight: bold;}
.search .check1 a:hover{color: #FFF; background: #29A7E2; border-radius: 5px;}
.search .check2 a:hover{color: #FFF; background: #545454; border-radius: 5px;}
.search div{float: right; border-left: 1px solid #DDD; padding-left: 10px; line-height: 26px;}
.search_input{display: block; float: left; width: 135px; background: none; border: none; height: 26px;}
.search_logo{display: block; float: left; background: url(../images/search.fw.png) center center no-repeat; width: 23px; height: 24px; border: none; cursor: pointer; margin-top: 2px;}
.search div b{color: #545454; font-weight: bold;}
.search div em{color: #29A7E2; font-weight: bold;}


.notice_main_problem{float: left; width: 100%; margin-top: 10px;}
.notice_main_problem ul{float: left; width: 100%; border-bottom: 1px dotted #CCC; line-height: 50px;}
.notice_main_problem ul li{width: 100%; float: left;}
.notice_main_problem ul li a{display: block; float: left; background: url(../images/notice_bg.fw.png) left center no-repeat; padding-left: 20px; color: #2B261B; font-size: 14px; max-width: 70%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.notice_main_problem ul li b{display:block; float: left; color: #29A7E2; margin-left: 8px;}
.notice_main_problem ul li em{display:block; float: left; color: #BBB; margin-left: 8px;}
.notice_main_problem ul li i{color: #FFBF00;display:block; float: left; margin-left: 8px;}
.notice_main_problem ul li strong{display: block; float: right; color: #CCC; font-size: 14px;}
.page1{text-align: right;margin-top: 10px; float: left; width: 100%;}


/*notice-details*/
.notice_details_main{float: left; padding: 30px 4%; width: 92%;}
.notice_details_main_problem{float: left; width: 100%; margin-top: 30px;}
.notice_details_main_problem>span{display: block; float: left; width: 100%; border-bottom: 1px solid #CCC; padding: 10px 0;}
.notice_details_main_problem>span strong{float: left; width: 100%; text-align: center;}
.notice_details_main_problem>span strong h2{color: #2B261B; font-size: 20px; width: 80%; margin: 0 auto; line-height: 28px;}
.notice_details_main_problem>span strong em{color: #CCC;}
.notice_details_main_problem>span strong b{color: #CCC; margin-right: 15px;}
.share{float: left;}
.share b{float: left; color: #CCC;}
.notice_details_main_problem>span>a{float: right; color: #28A6E1; font-weight: bold;}

.notice_content{float: left; width: 98%; padding: 10px 1%; margin-bottom: 20px;}  
.notice_btns{float: left; width: 100%; margin-top: 10px; border-top: 1px solid #ccc; padding: 10px 0;}
.notice_btns span{display: block; width: 40%; margin-bottom: 20px;}
.notice_btns span b{color: #2B2619;}
.notice_btns span a{color: #2B2619;}
.notice_btns_up{float: left;}
.notice_btns_down{float: right; text-align: right;}


/*product*/
/*小三角*/
.triangle{ position: absolute; z-index: 2; top: 20px; left: 0px;}
.triangle0{ position: absolute; z-index: 2; top: 19px; left: 0px;}
.triangle1{width: 0; height: 0; border-top: 3px solid #29A7E2; border-left: 3px solid transparent;}
.triangle2{width: 0; height: 0; border-top: 3px solid #E02665; border-left: 3px solid transparent;}
/*产品详情*/
span.pro_intro{float: left; margin-top: 10px; width: 220px;}
span.pro_intro strong{float: left; width: 100%; line-height: 24px;}
span.pro_intro strong em{color: #878787;}
span.pro_intro strong b{color: #333;}
span.pro_intro strong i{color: #111; font-weight: bold;}

/*站中*/
.pro_main{float: left; padding: 30px 4%; width: 92%;}

.pro_main_list{float: left; width: 103%; margin-top: 20px;}
.pro_main_list>a{display: block; float: left; width: 219px; border: 2px solid #e1e1e1; background: #FFF; border-radius: 5px; margin: 0 16px 16px 0; padding: 10px;}
.pro_main_list a>strong{display: block; border-bottom: 1px solid #eee; height: 125px; position: relative; text-align: center;}
.pro_main_list a>strong i{display: block; padding: 1px 12px; float: left; /*margin: -5px 0 0 -13px;*/ position: absolute; top: -5px; left: -14px; color: #FFF; font-size: 13px;}
.pro_main_list a>strong i.type1{background: #29A7E2;}
.pro_main_list a>strong i.type2{background: #E02665;}
.pro_main_list a>strong div{display: inline-block; height: 100%; vertical-align: middle;}
.pro_main_list a>strong img{vertical-align: middle;}
.pro_main_list a>strong em{color: #00B200; position: absolute;z-index: 2; bottom: -7px; right: 0; display: block; background: #FFF; padding-left: 8px;}
.page{width:97.6%; float: left;}
.page>div{float: right;}



.pro_list_main_details{float: left; width: 100%; margin-top: 20px;}
.pro_list_main{float: left; padding: 30px 4%; width: 92%;}





/*在线客服*/
.online{position: fixed; z-index: 9; width: 265px; right:0px; top:150px;}
.online>span{float: left; height: 260px; display: block; background: #27A5E0; float: right; border-radius: 5px 0 0 5px;}
.online_show{display: block; background: url(../images/service_bg.fw.png) center top no-repeat,url(../images/out.fw.png) center bottom no-repeat; width: 30px; font-size: 16px; color: #FFF; padding: 24px 0; text-align: center; float: left; margin: 60px 5px; cursor: pointer;}
.hotline{float: right; background: #27A5E0; width: 208px; height: 260px; border-top-left-radius: 5px; border-bottom-left-radius: 5px; display: none;}
.hotline>span{float: left;}
.online_hidden{display: block; background: url(../images/service_bg.fw.png) center top no-repeat,url(../images/in.fw.png) center bottom no-repeat; width: 30px; font-size: 16px; color: #FFF; padding: 24px 0; text-align: center; float: right; margin: 60px 5px; cursor: pointer;}
.hotline div{float: left; width: 150px; background: #FFF; margin-left: 5px; border-radius: 5px; margin-top: 12px;}
.hotline div>em{display: block; font-weight: bold; font-size: 13px; width: 100%; float: left; margin-top: 10px; text-align: center;}
.hotline div ul{float: left; width: 100%; border-bottom: 1px solid #e8e8e8; line-height: 30px; margin-top: 5px;}
.hotline div ul li{float: left; width: 50%; text-align: center;}
.hotline div ul li em{color: #BBB;}
.hotline div ul li a{float: left; margin-top: 6px;}
.hotline div span{float: left; display: block; width: 100%; border-bottom: 1px solid #e8e8e8;}
.hotline div span a{display: block; float: left; color: #BBB; line-height: 40px; width: 50%; text-align: center;}
.hotline div strong{float: left; width: 100%; text-align: center; line-height: 40px;}










/*站尾*/
.footer{background: #313131; float: left; width: 100%; padding: 20px 0 30px 0;}
.footer_logo{display: block; float: left; width: 21%; border-right: 1px solid #1D1D1D; height: 150px; padding: 10px 0; text-align: center; overflow: hidden;}
.footer_notic{float: left; width: 17%; border-left: 1px solid #414141; border-right: 1px solid #1D1D1D; height: 150px; padding: 10px 0 10px 2%; overflow: hidden;}
.footer_notic b{float: left; color: #FFF; font-size: 14px;}
.footer_notic span{float: left; display: block; width: 100%; margin-top: 17px;}
.footer_notic span a{color: #999;display: block; width: 125px; overflow: hidden; text-overflow: ellipsis;  white-space: nowrap; background: url(../images/dhbg.fw.png) left center no-repeat; text-indent: 43px; line-height: 32px; margin-bottom:10px; color: #ABCAD6;}

.footer_notic strong{display: block; float: left; width: 100%; margin-top: 11px;}
.footer_notic strong a{display: block; background: url(../images/footer_bg.png) left center no-repeat; padding-left: 10px; color: #999; line-height: 30px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; width: 90%;}

.footer_code{float: left; width: 19%; height: 150px; padding: 10px 2% 10px 0; overflow: hidden; text-align: right;}
.footer_code em{display: block; width: 100%; color: #999; font-size: 14px; line-height: 20px;}
.footer_code img{margin-top: 8px; float: right;}

.web_all>em{display: block; float: left; width: 100%; background: #252525; text-align: center; color: #BBB; padding: 10px 0;}


/*翻页*/

.prev,.next{float: left; padding: 4px 8px; margin-right: 6px;  color: #000;   border: 1px solid #CCCCCC;}
.num{float: left; padding: 3px 8px; list-style: none; margin-right: 6px;color: #000; border: 1px solid #CCCCCC;}
.rows{display: block; float: left; padding: 4px 8px; list-style: none; margin-right: 6px; border: none;}
.current{background: #29A7E2; float: left; padding: 3px 8px; list-style: none; margin-right: 6px; border: 1px solid #29A7E2; color: white;}