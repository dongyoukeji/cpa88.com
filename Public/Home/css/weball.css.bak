/*home站中*/
.main{float: left; padding: 30px;}
.main_product{width: 102%; float: left;}
/*.main_product a:hover{box-shadow: 0 0 8px #27A5E0;}*/
a.product{display: block; width: 245px; float: left; height: 140px; border: 1px solid #EAEAEA; background: #FFF; line-height: 140px; border-radius: 5px; position: relative; margin:0 20px 20px 0;}
a.product b{padding: 3px 8px; background: #29A7E2;; color: #FFF; position: absolute; z-index: 2; top: 5px; left: -3px; line-height: 14px;}
a.product>span{padding: 3px 8px; background: #E02665;; color: #FFF; position: absolute; z-index: 2; top: 5px; left: -3px; line-height: 12px; display: none}
a.product strong{display: block; text-align: center; vertical-align: middle;}
a.product strong img{vertical-align: middle;}
a.product .main_product_tx img{
    transition-delay: 0s;
    transition-duration: 1s;
    transition-property: all;
    transition-timing-function: ease;

    -webkit-transition-delay: 0s;
    -webkit-transition-duration: 1s;
    -webkit-transition-property: all;
    -webkit-transition-timing-function: ease;

    -moz-transition-delay: 0s;
    -moz-transition-duration: 1s;
    -moz-transition-property: all;
    -moz-transition-timing-function: ease;
}
a.product .main_product_tx:hover img{
    transform: scale(1.2, 1.2);
    -ms-transform:scale(1.2, 1.2);
    -moz-transform:scale(1.2, 1.2);
    -webkit-transform:scale(1.2, 1.2);
    -o-transform:scale(1.2, 1.2); 
}
a.product em{position: absolute; z-index: 2; bottom: 10px; left: 10px; line-height: 12px; color: #BABABA;}
a.product>i{position: absolute; z-index: 2; bottom: 10px; right: 10px; line-height: 12px; font-family: "宋体"; color: #999; font-weight: bold;}
a.product p{position: absolute; z-index: 2; bottom: 10px; right: 10px; line-height: 12px; font-family: "宋体"; color: #00B200;}
